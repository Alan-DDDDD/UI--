{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/test/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/debug/(.*)",
      "dest": "/server.js"
    }
  ]
}